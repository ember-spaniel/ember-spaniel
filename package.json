{
  "name": "ember-spaniel",
  "version": "0.7.4",
  "description": "Ember addon wrapping spaniel and providing viewport and requestAnimationFrame utilities.",
  "keywords": [
    "ember-addon",
    "viewport",
    "requestAnimationFrame"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ember-spaniel/ember-spaniel"
  },
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "yarn lint:hbs && yarn lint:js",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "yarn lint && ember test",
    "test:all": "ember try:each",
    "prepare": "node prepublish.js",
    "nuke": "rm -rf tmp node_modules dist pre-built"
  },
  "dependencies": {
    "calculate-cache-key-for-tree": "^2.0.0",
    "ember-cli-babel": "^7.7.3",
    "ember-rollup": "^3.0.0",
    "spaniel": "^2.7.4"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.4.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.27.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-htmlbars": "^5.3.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^2.0.2",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "^3.27.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-try": "^1.0.0",
    "ember-welcome-page": "^4.0.0",
    "eslint": "^7.12.1",
    "eslint-plugin-ember": "^9.5.0",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "qunit": "^2.16.0",
    "qunit-dom": "^1.5.0"
  },
  "engines": {
    "node": "10.* || >= 12.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
